{
    "compilerOptions": {
        "sourceMap": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": false,
        "jsx": "react-jsxdev",
        "paths": {
            "react/jsx-runtime": [
                "../engine/bin/*"
            ]
        },
        "noImplicitReturns": false,
        "strictNullChecks": false
    },
    "include": [
        "..//**/*",
        "../engine/**/*"
    ]
}